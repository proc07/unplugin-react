{
  "name": "unplugin-react",
  "author": "Proc Zhang<790847400@qq.com>",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@8.6.12",
  "description": "",
  "license": "MIT",
  "homepage": "https://github.com/proc07/unplugin-react#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/proc07/unplugin-react.git"
  },
  "bugs": {
    "url": "https://github.com/proc07/unplugin-react/issues"
  },
  "keywords": [
    "react",
    "sfc",
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./rspack": {
      "types": "./dist/rspack.d.ts",
      "import": "./dist/rspack.js",
      "require": "./dist/rspack.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint .",
    "play": "npm -C playground run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "rollup": "^3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "webpack": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    }
  },
  "dependencies": {
    "@babel/core": "^7.24.4",
    "@babel/plugin-transform-react-jsx-self": "^7.24.1",
    "@babel/plugin-transform-react-jsx-source": "^7.24.1",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@vue/compiler-dom": "^3.4.25",
    "@vue/compiler-sfc": "^3.4.21",
    "react-refresh": "^0.14.0",
    "unplugin": "^1.5.1",
    "vite": "^5.0.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.2.2",
    "@types/babel__core": "^7.20.5",
    "@types/node": "^20.10.3",
    "bumpp": "^9.4.0",
    "chalk": "^5.3.0",
    "esbuild": "0.20.2",
    "eslint": "^9.0.0",
    "esno": "^4.0.0",
    "fast-glob": "^3.3.2",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "rollup": "^4.14.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "slash": "^5.1.0",
    "source-map-js": "^1.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.4",
    "vitest": "1.4.0",
    "webpack": "^5.91.0"
  }
}
